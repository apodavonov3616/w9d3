<h1>Search!</h1>

<nav class='users-search'>
  <input class='user-search-input'
    type='text'
    name='name'
    placeholder = 'Enter a name'>
  >
</nav>
<ul>
  <% @users.each do |user| %>
    <li>
      <a href="<%= user_url(user) %>"><%= user.username %></a>
      <%= render 'follows/form', user: user %>
    </li>
  <% end %>
</ul>
